{# Purpose of this template: Display avatars within an external context #}
{% for avatar in items %}
    <h3>{{ avatar|muimagemodule_formattedTitle }}</h3>
    <p><a href="{{ path('muimagemodule_avatar_display', {'id': avatar.getId()}) }}" title="{{ __('Read more')|e('html_attr') }}">{{ __('Read more') }}</a>
    </p>
{% endfor %}
