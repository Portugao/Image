{
    "name": "mu/image-module",
    "version": "1.4.0",
    "description": "Image Pictures in Albums and SubAlbums",
    "type": "zikula-module",
    "license": "LGPL-3.0+",
    "authors": [
        {
            "name": "Michael Ueberschaer",
            "email": "kontakt@webdesign-in-bremen.com",
            "homepage": "http://www.webdesign-in-bremen.com",
            "role": "owner"
        }
    ],
    "autoload": {
        "psr-4": { "MU\\ImageModule\\": "" }
    },
    "require": {
        "php": ">=5.4.1"
    },
    "suggest": {
    },
    "extra": {
        "zikula": {
            "core-compatibility": ">=1.4.5 <3.0",
            "class": "MU\\ImageModule\\MUImageModule",
            "displayname": "Image",
            "url": "image",
            "oldnames": ["MUImage"],
            "capabilities": {
                "user": {"route": "muimagemodule_user_index"},
                "admin": {"route": "muimagemodule_admin_index"},
                "searchable": {"class": "MU\\ImageModule\\Helper\\SearchHelper"},
                "categorizable": {
                    "entities": [
                        "MU\\ImageModule\\Entity\\AlbumEntity",
                        "MU\\ImageModule\\Entity\\AvatarEntity"
                    ]
                },
                "hook_subscriber": {"class": "MU\\ImageModule\\Container\\HookContainer"}
            },
            "securityschema": {
                "MUImageModule::": "::",
                "MUImageModule:ItemListBlock:": "Block title::",
                "MUImageModule:ModerationBlock:": "Block title::",
                "MUImageModule:Album:": "Album ID::",
                "MUImageModule:Album:Album": "Album ID:Album ID:",
                "MUImageModule:Picture:": "Picture ID::",
                "MUImageModule:Album:Picture": "Album ID:Picture ID:",
                "MUImageModule:Avatar:": "Avatar ID::",
                "MUImageModule::Ajax": "::"
            }
        }
    }
}
